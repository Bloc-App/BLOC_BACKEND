{
  "name": "lasrerams",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon --exec node ./bin/www",
    "nyc": "nyc --reporter=lcov npm run test && nyc report --reporter=text-lcov",
    "test": "set NODE_ENV=test && npm run migrate:table  && npm run seed:table  && nyc --reporter=text mocha --timeout 10000 -c ./server/test/**.test.js  --exit",
    "seed:table": "knex seed:run",
    "migrate:table": "knex migrate:latest ",
    "reset:db": "knex rollback:migrate && knex migrate:latest && knex seed:run"
  },
